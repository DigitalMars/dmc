LIBRARY		"TRACE"

DESCRIPTION	'"TRACE Project"'

EXETYPE		NT

SUBSYSTEM	WINDOWS

STUB		'WINSTUB.EXE'

CODE		 EXECUTE READ

DATA		 READ WRITE

HEAPSIZE	1048576,4096 

EXPORTS
		PromptTraceFlags @2
		FilterDllMsg @3
		ProcessDllIdle @4
